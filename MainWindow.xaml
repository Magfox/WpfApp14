<Window x:Class="WpfApp14.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp14"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <DataGrid x:Name="helpDeskDataGrid" AutoGenerateColumns="False">
            <DataGrid.Columns>
                <DataGridTextColumn Header="Агенты" Binding="{Binding Agents}" />
                <DataGridTextColumn Header="Пользователи" Binding="{Binding Users}" />
                <DataGridTextColumn Header="Статус" Binding="{Binding Statuses}" />
                <DataGridTextColumn Header="Описание" Binding="{Binding Reports}" />
                <DataGridTextColumn Header="Дата создания" Binding="{Binding Tickets}" />
            </DataGrid.Columns>
        </DataGrid>
    </Grid>
</Window>
